    "lint": "ng lint",
    "e2e": "ng e2e",
    "cap": "npm run build && npx cap copy android && npx cap update android && npx cap copy ios && npx cap update ios",
    "build-worker": "ng run app:build-worker",
    "dev-worker": "ng run app:dev-worker",
    "electron-patch": "node electron-patch.js",
    "electron-build": "electron-builder"
  },
